{
  "name": "@microsofthackathons/storybook-fast-components",
  "author": "Microsoft",
  "version": "0.0.1",
  "description": "Storybook fast-components renderer",
  "keywords": [
    "fast",
    "fast-element",
    "storybook",
    "web-components",
    "fast-components"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microsofthackathons/fast-storybook.git",
    "directory": "packages/renderers/fast-components"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./render": "./dist/render.js",
    "./preset": "./preset.js",
    "./dist/entry-preview.mjs": "./dist/entry-preview.mjs",
    "./dist/entry-preview-docs.mjs": "./dist/entry-preview-docs.mjs",
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "template/cli/**/*",
    "README.md",
    "*.js",
    "*.d.ts",
    "!src/**/*"
  ],
  "dependencies": {
    "@microsoft/fast-element": "^2.0.0",
    "@storybook/cli": "^8.3.0",
    "@storybook/components": "^8.3.0",
    "@storybook/core": "^8.3.0",
    "@storybook/csf": "^0.1.11",
    "@storybook/global": "^5.0.0",
    "@storybook/manager-api": "^8.3.0",
    "@storybook/preview-api": "^8.3.0",
    "@storybook/theming": "^8.3.0",
    "tiny-invariant": "^1.3.1",
    "dedent": "^1.5.3",
    "prettier": "^3.3.3"
  },
  "type": "module",
  "devDependencies": {
    "web-component-analyzer": "^2.0.0"
  },
  "peerDependencies": {
    "@microsoft/fast-element": "^2.0.0",
    "storybook": "^8.3.0"
  },
  "peerDependenciesMeta": {
    "@microsoft/fast-element": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "npm run build:tsc",
    "build:tsc": "tsc -p tsconfig.build.json",
    "watch": "npm run build:tsc -- -w"
  }
}